        "import random\n",
        "\n",
        "def generate_otp(length=4):\n",
        "    otp = \"\"\n",
        "    for _ in range(length):\n",
        "        otp += str(random.randint(0, 9))\n",
        "    return otp\n",
        "\n",
        "# Generate a 6-digit OTP\n",
        "otp = generate_otp()\n",
        "print(\"Generated OTP:\", otp)\n"
      ],
